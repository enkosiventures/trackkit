{
  "name": "trackkit-monorepo",
  "packageManager": "pnpm@10.13.1",
  "private": true,
  "scripts": {
    "build:types": "pnpm -r run build:types",
    "defaults:inject": "pnpm -r run defaults:inject",
    "defaults:assert": "pnpm -r run defaults:assert",
    "test": "pnpm -r run test",
    "test:coverage": "pnpm -r run test:coverage",
    "deadcode": "pnpm -r run deadcode",
    "deadcode:ci": "pnpm -r run deadcode:ci",
    "prebuild": "pnpm -r run prebuild",
    "build": "pnpm -r run build",
    "typecheck": "pnpm -r run typecheck",
    "lint": "eslint . --ext .ts,.tsx --cache",
    "size": "pnpm -r run size",
    "clean": "pnpm -r run clean",
    "check": "scripts/check.sh",
    "check:quick": "scripts/check.sh --quick",
    "check:docs": "scripts/check.sh --docs",
    "docs:gen:sdk": "pnpm --filter trackkit run typedoc",
    "docs:dev": "vitepress dev docs",
    "docs:preview": "vitepress preview docs",
    "docs:build": "pnpm -s docs:gen:sdk && vitepress build docs"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "gh-pages": "^6.3.0",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "knip": "^5.63.1",
    "linkinator": "^7.1.3",
    "msw": "^2.10.4",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typedoc": "^0.28.13",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.5.4",
    "vitepress": "^1.6.4",
    "vitest": "^3.2.4"
  }
}
