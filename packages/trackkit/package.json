{
  "name": "trackkit",
  "version": "0.0.1",
  "description": "Lightweight, provider-agnostic analytics SDK",
  "keywords": [
    "analytics",
    "tracking",
    "umami",
    "plausible",
    "google-analytics"
  ],
  "homepage": "https://github.com/enkosiventures/trackkit",
  "bugs": "https://github.com/enkosiventures/trackkit/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enkosiventures/trackkit.git",
    "directory": "packages/trackkit"
  },
  "license": "MIT",
  "author": "Enkosi Ventures <enkosiventures@gmail.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./ssr": {
      "import": {
        "types": "./dist/ssr.d.ts",
        "default": "./dist/ssr.js"
      },
      "require": {
        "types": "./dist/ssr.d.cts",
        "default": "./dist/ssr.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "node ../../scripts/generate-env-types.mjs",
    "build": "tsup --config tsup.config.ts && tsc -p tsconfig.build.json",
    "build:watch": "tsup --config tsup.config.ts --watch",
    "build:prod": "tsup --config tsup.config.ts",
    "build:types": "tsc -p tsconfig.types.json",
    "defaults:inject": "pnpm run build:types && pnpm exec sync-doc-defaults inject",
    "defaults:assert": "pnpm exec sync-doc-defaults assert",
    "typedoc": "typedoc --options typedoc.json",
    "clean": "rimraf dist coverage",
    "lint": "eslint src test --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "coverage:check": "node ../../scripts/coverage-check.mjs",
    "deadcode:only": "node ../../scripts/deadcode-report.mjs --workspace --ensure-coverage",
    "deadcode": "node ../../scripts/deadcode-report.mjs --workspace --ensure-coverage",
    "deadcode:types": "node ../../scripts/deadcode-report.mjs --workspace --ensure-coverage --include-statementless",
    "deadcode:ci": "node ../../scripts/deadcode-report.mjs --ensure-coverage",
    "deadcode:gate": "node ../../scripts/deadcode-report.mjs --ensure-coverage --fail-on-unused --max-uncovered-files=0",
    "verify": "pnpm -s test:coverage && pnpm -s coverage:check && pnpm -s deadcode:gate",
    "knip": "knip",
    "size": "size-limit",
    "prepublishOnly": "pnpm run clean && pnpm run build && pnpm run defaults:inject && pnpm run defaults:assert"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.2.0",
    "@types/node": "^24.0.13",
    "@vitest/coverage-v8": "^3.2.4",
    "jsdom": "^26.1.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.45.0",
    "rollup-plugin-esbuild": "^6.2.1",
    "size-limit": "^11.2.0",
    "sync-doc-defaults": "^0.1.3",
    "terser": "^5.43.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typedoc": "^0.28.13",
    "typescript": "^5.5.4",
    "vitest": "^3.2.4"
  }
}
