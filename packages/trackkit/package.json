{
  "name": "trackkit",
  "version": "0.0.1",
  "description": "Lightweight, provider-agnostic analytics SDK",
  "keywords": [
    "analytics",
    "tracking",
    "umami",
    "plausible",
    "google-analytics"
  ],
  "homepage": "https://github.com/enkosiventures/trackkit",
  "bugs": "https://github.com/enkosiventures/trackkit/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enkosiventures/trackkit.git",
    "directory": "packages/trackkit"
  },
  "license": "MIT",
  "author": "Enkosi Ventures <enkosiventures@gmail.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup --tsconfig tsconfig.build.json",
    "build:watch": "tsup --watch --tsconfig tsconfig.build.json",
    "clean": "rimraf dist coverage",
    "lint": "eslint src test --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "size": "size-limit",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.2.0",
    "@types/node": "^24.0.13",
    "@vitest/coverage-v8": "^3.2.4",
    "rimraf": "^6.0.1",
    "rollup": "^4.45.0",
    "rollup-plugin-esbuild": "^6.2.1",
    "size-limit": "^11.2.0",
    "terser": "^5.43.1",
    "tsup": "^8.5.0",
    "typescript": "^5.5.4",
    "vitest": "^3.2.4"
  }
}
